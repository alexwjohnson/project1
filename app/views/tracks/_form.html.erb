<%= form_for(@track, :html => {:multipart => true}) do |f| %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :catalog %>
    <%= f.text_field :catalog %>

    <%= f.label :year %>
    <%= f.text_field :year %>

    <%= f.label :bpm %>
    <%= f.text_field :bpm %>
    
    <%= f.label :music_key %>
    <%= f.text_field :music_key %>
    
    <%= f.label :duration %>
    <%= f.text_field :duration %>

    <%= f.label :rmx %>
    <%= f.text_field :rmx %>

    <%= f.label :image %>
    <%= f.cl_image_upload :image, :required => true %>




    <%= f.label :artist_id %>
    <%= f.select :artist_id, Artist.pluck(:name, :id), :include_blank => true  %>

    <%= f.submit %>

<% end %>